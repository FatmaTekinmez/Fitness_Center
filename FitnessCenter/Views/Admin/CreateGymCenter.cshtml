@model FitnessCenter.Models.GymCenter

<h2>Yeni Gym Ekle</h2>

<form asp-action="CreateGymCenter" method="post">
    <div>
        <label>Gym Adı</label>
        <input asp-for="Name" />
        <span asp-validation-for="Name"></span>
    </div>

    <div>
        <label>Çalışma Saatleri</label>
        <input asp-for="WorkingHours" />
        <span asp-validation-for="WorkingHours"></span>
    </div>

    <div>
        <label>Adres</label>
        <input asp-for="Address" />
        <span asp-validation-for="Address"></span>
    </div>

    <button type="submit">Kaydet</button>
</form>

 
